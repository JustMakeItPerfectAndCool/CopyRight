function DistanceCheck(x,y,z, distance)
	local PlayerPosition = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
	local Destination = Vector3.new(x,y,z)
	return (PlayerPosition-Destination).magnitude < distance
end
function CreatePlat(seconds)
	local PlayerRoot = game.Players.LocalPlayer.Character.HumanoidRootPart
	local Plat = Instance.new("Part", game.Workspace)
	Plat.Anchored = true
	Plat.CFrame = CFrame.new(PlayerRoot.CFrame.X, PlayerRoot.CFrame.Y-3, PlayerRoot.CFrame.Z)
	Plat.Size = Vector3.new(1000, 1, 1000)
	Plat.Transparency = 1
	task.spawn(function()
		task.wait(seconds)
		Plat:Destroy()
	end)
end

function TeleportToBase()
	success = false
	local vim = game:service("VirtualInputManager")
	hb = game:GetService("RunService").Heartbeat:Connect(function()
		if not success then
	    	if not game.Players.LocalPlayer.Character.Humanoid.SeatPart then
	    	    if not success then
	    	        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-91, 25.4, -173)
	    	        vim:SendKeyEvent(true, "E", false, game)
	    	        task.wait(0.1)
	    	        vim:SendKeyEvent(false, "E", false, game)
	    	    end
	    	else
	    	    success = true
	    	    repeat
	    	    vim:SendKeyEvent(true, "Space", false, game)
	    	    task.wait(0.1)
	    	    vim:SendKeyEvent(false, "Space", false, game)
	    	    until not game.Players.LocalPlayer.Character.Humanoid.SeatPart
	    	end
		end
	end)
	repeat wait() until success
	for i = 1, 10 do
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-91, 50, -173)
		task.wait(0.001)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-91, 25.4, -173)
		task.wait(0.001)
	end
	task.wait(0.5)
end



function FastTeleport(x, y, z, speed, loops)
	local targetPosition = Vector3.new(x, y, z)
	local speed = speed
	local maxForce = Vector3.new(math.huge, math.huge, math.huge)

	local velocity = Instance.new("BodyVelocity")
	velocity.MaxForce = maxForce
	velocity.Velocity = Vector3.new(0, 0, 0)
	velocity.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart

	while true do
		if canceltp then
			canceltp = false
			velocity:Destroy()
			break
		end
		local currentPos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
		local direction = (targetPosition - currentPos).unit
		local distance = (targetPosition - currentPos).magnitude
		local distanceCheck = speed / 30
		if distance >= distanceCheck then
			velocity.Velocity = direction * speed
		else
			velocity:Destroy()
			if loops then
				for i = 1, loops do
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition)
					game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
					task.wait()
				end
			else
				for i = 1, 10 do
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition)
					game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
					task.wait()
				end
			end
			break
		end
		wait()
	end
end
local vim = game:service("VirtualInputManager")
function Click(key, HT, cd)
	vim:SendKeyEvent(true, key, false, game)
	task.wait(HT)
	vim:SendKeyEvent(false, key, false, game)
	task.wait(cd)
end
function InstaTeleport(x, y, z)
	local LPRootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
	tp(LPRootPart.CFrame.X, 1000, LPRootPart.CFrame.Z, 100000)
	tp(x, 1000, z, 100000)
	tp(x, y, z, 100000)
end

